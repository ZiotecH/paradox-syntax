{
    "contributors": [
        "Gratak",
        "Dayshine",
        "Danboy Bebace"
    ],
    "name": "paradox-syntax",
    "displayName": "Paradox Syntax Highlighting",
    "description": "Modified version for personal use.",
    "version": "0.1.12",
    "publisher": "tboby",
    "repository": "https://github.com/tboby/paradox-syntax",
    "preview": true,
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "hoi4"
            },
            {
                "id": "stellaris"
            },
            {
                "id": "ck2"
            },
            {
                "id": "eu4"
            },
            {
                "id": "imperator"
            },
            {
                "id": "vic2"
            },
            {
                "id": "ck3"
            },
            {
                "id": "paradox"
            }
        ],
        "grammars": [
            {
                "language": "stellaris",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "scopeName": "source.stellaris",
                "injectTo": [
                    "source.mod"
                ],
                "path": "./syntaxes/stellaris.tmLanguage.json"
            },
            {
                "language": "hoi4",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "scopeName": "source.hoi4",
                "injectTo": [
                    "source.mod"
                ],
                "path": "./syntaxes/hoi4.tmLanguage.json"
            },
            {
                "language": "eu4",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "scopeName": "source.eu4",
                "injectTo": [
                    "source.mod"
                ],
                "path": "./syntaxes/eu4.tmLanguage.json"
            },
            {
                "language": "ck2",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "scopeName": "source.ck2",
                "injectTo": [
                    "source.mod"
                ],
                "path": "./syntaxes/ck2.tmLanguage.json"
            },
            {
                "language": "imperator",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "language": "vic2",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "language": "ck3",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "language": "paradox",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            }
        ],
        "snippets":[
            {
                "language": "stellaris",
                "path": "./snippets/stellaris.tmLanguage.json"
            }
        ],
        "themes": [
            {
                "label": "Stellaris Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/Paradox-Dimmed.tmLanguage.json"
            },
            {
                "label": "Quiet Light Paradox Theme",
                "uiTheme": "vs",
                "path": "./themes/Paradox-QuietLight.tmLanguage.json"
            }
        ]
    }
}
